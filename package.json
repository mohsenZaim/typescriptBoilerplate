{
	"name": "base-boilerplate",
	"version": "20.07.11",
	"description": "Boilerplate with React and Typescript",
	"author": "Mohsen Asle Zaim",
	"license": "ISC",
	"scripts": {
		"build:dev": "webpack-dev-server --config webpack.config.dev.js --host 0.0.0.0",
		"build:production": "webpack --config webpack/webpack.config.prod.js",
		"build:staging": "webpack --config webpack/webpack.config.staging.js",
		"clean": "rimraf ./dist",
		"lint": "tslint -p tsconfig.json",
		"test": "rimraf ./tests/coverage && jest",
		"start": "npm run build:dev",
		"build": "npm run clean && npm run lint && npm run build:production",
		"staging": "npm run clean && npm run lint && npm run build:staging",
		"swagger": "swagger-codegen generate -i http://petstore.swagger.io/v2/swagger.json -l typescript-node -t ./templates_mustache/TypescriptNode -o ./src/apis/generated",
		"nswag": "nswag openapi2tsclient /input:http://petstore.swagger.io/v2/swagger.json /output:src/apis/generated/api.ts /runtime:NetCore31 /template:Axios /typeStyle:interface /templateDirectory:./templates_liquid"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"moment": "^2.27.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-helmet": "^6.1.0",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0"
	},
	"devDependencies": {
		"@types/enzyme": "^3.10.5",
		"@types/jest": "^26.0.4",
		"@types/jsdom": "^16.2.3",
		"@types/node": "^14.0.22",
		"@types/react": "^16.9.42",
		"@types/react-dom": "^16.9.8",
		"@types/react-router": "^5.1.8",
		"@types/systemjs": "^6.1.0",
		"autoprefixer": "^9.8.5",
		"babel-polyfill": "^6.26.0",
		"case-sensitive-paths-webpack-plugin": "^2.3.0",
		"copy-webpack-plugin": "^6.0.3",
		"css-loader": "^3.6.0",
		"dotenv-webpack": "^2.0.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"file-loader": "6.0.0",
		"html-webpack-plugin": "4.3.0",
		"jest": "^26.1.0",
		"jest-enzyme": "^7.1.2",
		"jsdom": "^16.3.0",
		"jsdom-global": "^3.0.2",
		"node-sass": "^4.14.1",
		"postcss-loader": "^3.0.0",
		"raw-loader": "^4.0.1",
		"sass-loader": "^9.0.2",
		"style-loader": "^1.2.1",
		"ts-jest": "^26.1.1",
		"ts-loader": "^8.0.0",
		"tslib": "^2.0.0",
		"tslint": "^6.1.2",
		"tslint-react": "^5.0.0",
		"typescript": "^3.9.6",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "^5.0.9"
	},
	"browserslist": [
		"defaults",
		"not ie < 11",
		"last 2 versions",
		"> 1%",
		"iOS 7",
		"last 3 iOS versions"
	]
}
