{
	"name": "base-boilerplate",
	"version": "20.04.23",
	"description": "Boilerplate with React and Typescript",
	"author": "Mohsen Asle Zaim",
	"license": "ISC",
	"scripts": {
		"build:dev": "webpack-dev-server --config webpack.config.dev.js --host 0.0.0.0",
		"build:production": "webpack --config webpack/webpack.config.prod.js",
		"build:staging": "webpack --config webpack/webpack.config.staging.js",
		"clean": "rimraf ./dist",
		"lint": "tslint -p tsconfig.json",
		"test": "rimraf ./tests/coverage && jest",
		"start": "npm run build:dev",
		"build": "npm run clean && npm run lint && npm run build:production",
		"staging": "npm run clean && npm run lint && npm run build:staging",
		"swagger": "swagger-codegen generate -i http://petstore.swagger.io/v2/swagger.json -l typescript-node -t ./template/TypescriptNode -o ./src/apis/generated"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"moment": "^2.24.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-helmet": "^6.0.0",
		"react-router": "^5.1.2",
		"react-router-dom": "^5.1.2"
	},
	"devDependencies": {
		"@types/enzyme": "^3.10.5",
		"@types/jest": "^25.2.1",
		"@types/jsdom": "^16.2.1",
		"@types/node": "^13.13.2",
		"@types/react": "^16.9.34",
		"@types/react-dom": "^16.9.6",
		"@types/react-router": "^5.1.5",
		"@types/systemjs": "^6.1.0",
		"autoprefixer": "^9.7.6",
		"babel-polyfill": "^6.26.0",
		"base-href-webpack-plugin": "^2.0.0",
		"case-sensitive-paths-webpack-plugin": "^2.3.0",
		"copy-webpack-plugin": "^5.1.1",
		"css-loader": "^3.5.2",
		"dotenv-webpack": "^1.7.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"file-loader": "6.0.0",
		"html-webpack-plugin": "3.2.0",
		"jest": "^25.4.0",
		"jest-enzyme": "^7.1.2",
		"jsdom": "^16.2.2",
		"jsdom-global": "^3.0.2",
		"node-sass": "^4.13.1",
		"postcss-loader": "^3.0.0",
		"raw-loader": "^4.0.1",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.1.4",
		"ts-jest": "^25.4.0",
		"ts-loader": "^7.0.1",
		"tslib": "^1.11.1",
		"tslint": "^6.1.1",
		"tslint-react": "^5.0.0",
		"typescript": "^3.8.3",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3",
		"webpack-merge": "^4.2.2"
	},
	"browserslist": [
		"defaults",
		"not ie < 11",
		"last 2 versions",
		"> 1%",
		"iOS 7",
		"last 3 iOS versions"
	]
}
